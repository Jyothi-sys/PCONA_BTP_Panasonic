PROCEDURE "USP_UPDATE_OPEN_QTY"(IN V_PO NVARCHAR(50),IN V_POLINE NVARCHAR(5),IN V_QTY DECIMAL(13,2))
   LANGUAGE SQLSCRIPT
   SQL SECURITY INVOKER
   --DEFAULT SCHEMA <default_schema_name>
   --READS SQL DATA 
   AS
BEGIN
UPDATE BTP_PANASONIC_A_PURCHASEORDERITEM Set SCHEDULELINEOPENQTY = V_QTY
Where PURCHASEORDER_PURCHASEORDER = V_PO and PURCHASEORDERITEM = V_POLINE ;

END