FUNCTION "GET_PO_ITEMID"(V_PO NVARCHAR(50),V_MATERIAL NVARCHAR(50))
    RETURNS V_POITEM NVARCHAR(10)
    LANGUAGE SQLSCRIPT
    SQL SECURITY INVOKER AS
BEGIN
Select MAX(T0.PURCHASEORDERITEM) INTO V_POITEM DEFAULT '00' FROM BTP_PANASONIC_A_PURCHASEORDERITEM As T0 Where T0.PURCHASEORDER_PURCHASEORDER = V_PO 
And T0.MATERIAL = V_MATERIAL ;
END;