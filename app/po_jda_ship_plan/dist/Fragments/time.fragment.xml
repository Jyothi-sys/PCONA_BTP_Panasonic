<c:FragmentDefinition
	xmlns="sap.m"
	xmlns:c="sap.ui.core">


  <SelectDialog  
  noDataText="{i18n>NoDataFound}"
  title="{i18n>PROC_TIME}"
  liveChange="onSearchTime"
  confirm="onSelectionTime"
  cancel="onDialogClose"
  multiSelect="true"
  items="{timeData>/}">

<StandardListItem 

title="{path: 'timeData>time'}"
iconDensityAware="false"
iconInset="false"
type="Active" />


</SelectDialog >
</c:FragmentDefinition>

